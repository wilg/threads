= title "Sign Up"

= twitter_bootstrap_form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => { :class => 'form-horizontal'}) do |f|
  = devise_error_messages!
  = f.text_field :name, "First + Last Name"
  = f.email_field :email
  = f.password_field :password
  = f.password_field :password_confirmation
  = f.actions do
    = f.submit "Sign up"


= render "devise/shared/links"